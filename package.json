{
  "name": "compare-npm-package",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build:browser:prod && npm run build:server:prod",
    "pre-build": "node src\\version.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "ngsw-config": "node_modules/.bin/ngsw-config dist\\compare-npm-package ./ngsw-config.json",
    "ngsw-copy": "copy node_modules\\@angular\\service-worker\\ngsw-worker.js dist\\compare-npm-package\\",
    "build-prod-ngsw-local": "ng build compare-npm-package --configuration=sw && npm run ngsw-config && npm run ngsw-copy",
    "serve-prod-ngsw-local": "npm run build-prod-ngsw-local && http-server dist\\compare-npm-package -a localhost -p 4200 -c-1 --ssl -C dist\\compare-npm-package\\ssl\\server.crt -K dist\\compare-npm-package\\ssl\\server.key",
    "build:browser:prod": "npm run pre-build && ng build --prod",
    "build:browser:serverless": "ng build --prod --base-href /production/",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "server": "node local.js",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy",
    "deploy": "serverless deploy",
    "build:server:prod": "webpack --config webpack.server.config.js --progress --colors",
    "build:server:serverless": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/pwa": "^0.8.4",
    "@angular/router": "^6.1.0",
    "@angular/service-worker": "^6.1.9",
    "@ng-toolkit/serverless": "^1.1.42",
    "@ng-toolkit/universal": "^1.1.42",
    "angular-highcharts": "^6.2.6",
    "core-js": "^2.5.4",
    "highcharts": "^6.1.4",
    "moment": "^2.22.2",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26",
    "cors": "~2.8.4",
    "cp-cli": "^1.1.0",
    "aws-serverless-express": "^3.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.2.4",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.9",
    "@compodoc/compodoc": "^1.1.5",
    "@types/highcharts": "^5.0.30",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "fs-extra": "^7.0.0",
    "git-describe": "^4.0.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "opencollective": "^1.0.3",
    "path": "^0.12.7",
    "protractor": "~5.3.0",
    "serverless": "1.26.1",
    "serverless-apigw-binary": "^0.4.4",
    "ts-loader": "4.2.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.9.2",
    "webpack-cli": "2.1.2"
  }
}
